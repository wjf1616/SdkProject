<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:state_checked="true"
        android:drawable="@drawable/mail_list_edit_check_box_checked" />
    <item android:state_pressed="true" android:state_checked="false"
        android:drawable="@drawable/mail_list_edit_check_box_unchecked" />
    <item android:state_checked="true" android:state_focused="true"
        android:drawable="@drawable/mail_list_edit_check_box_checked" />
    <item android:state_checked="false" android:state_focused="true"
        android:drawable="@drawable/mail_list_edit_check_box_unchecked" />
    <item android:state_checked="false"
        android:drawable="@drawable/mail_list_edit_check_box_unchecked" />
    <item android:state_checked="true"
        android:drawable="@drawable/mail_list_edit_check_box_checked" />
</selector>